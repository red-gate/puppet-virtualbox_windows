$server_properties = (& 'C:\Program Files\Oracle\VirtualBox\vboxmanage' list -l dhcpservers) `
  -join '`n' `
  -split '`n`n' `
  -like 'NetworkName:*<%=@dhcp_netname-%>*' `
  -split '`n'

if(!($adapter_properties -like 'IP:*<%= @dhcp_ipaddress-%>')) {
  # IP is incorrect. return 0
  exit 0
}

if(!($adapter_properties -like 'lowerIPAddress:*<%= @dhcp_lower_ip_range-%>')) {
  # Lower IP range is incorrect. return 0
  exit 0
}

if(!($adapter_properties -like 'upperIPAddress:*<%= @dhcp_upper_ip_range-%>')) {
  # Upper IP range is incorrect. return 0
  exit 0
}

if(!($adapter_properties -like 'Enabled:*True')) {
  # not enabled. return 0
  exit 0
}

# Properties are all good. return 1
exit 1
